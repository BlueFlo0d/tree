(pushnew (ccl:current-directory) ql:*local-project-directories*)
(declaim (optimize (debug 3)))

(ql:quickload "tree")
(in-package :tree)
(format t "Please specifiy player ID:")
(defparameter *cli-player-id* (read))
(format t "~%")
(defparameter *enable-audio-p* (eq *cli-player-id* 'audio))
(when *enable-audio-p*
  (server-boot *s*)
  (setup-server))
(defvar master-tree)
(setq master-tree (make-instance 'tree :player-id *cli-player-id*))
(defun master-tree-node-dump ()
  (node-base-dump (slot-value master-tree 'node-base)))
(defun master-tree-node-load (l)
  (tree-load master-tree l))
